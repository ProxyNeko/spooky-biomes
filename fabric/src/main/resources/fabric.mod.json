{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${display_url}",
    "sources": "${mod_source}",
    "issues": "${mod_source}/issues"
  },
  "license": "${license}",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [ "${mod_group}.MysteriousFabric" ],
    "fabric-datagen": [ "${mod_group}.datagen.MysteriousDataGen" ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "geckolib": "${geckolib_version_fabric}",
    "java": "${java_version_range}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord}",
        "modmenu.modrinth": "${modrinth_url}",
        "modmenu.curseforge": "${curseforge_url}"
      },
      "update_checker": true
    }
  }
}
